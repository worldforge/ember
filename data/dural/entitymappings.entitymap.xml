    <entitymapping name="human">
        <entitymatch>
            <case>
                <caseparam type="equals">human</caseparam>
                <action type="display-label" />
                <action type="display-model">male</action>
                <action type="display-part">bare_torso</action>
                <action type="display-part">bare_legs</action>
                <action type="display-part">bare_feet</action>
                <outfitmatch attachment="body">
                    <case>
                        <caseparam type="equals">shirt</caseparam>
                        <action type="display-part">basic_shirt</action>
                        <attributematch attribute="style">
                            <case>
                                <caseparam type="equals">burlap</caseparam>
                                <action type="display-part">basic_shirt</action>
                            </case>
                            <case>
                                <caseparam type="equals">chain</caseparam>
                                <action type="display-part">basic_chain_shirt</action>
                            </case>
                            <case>
                                <caseparam type="equals">ossyja</caseparam>
                                <action type="display-part">ossyja_shirt</action>
                            </case>
                        </attributematch>
                    </case>
                </outfitmatch>                
                <outfitmatch attachment="feet">
                    <case>
                        <caseparam type="equals">boots</caseparam>
                        <action type="display-part">footwear</action>
                    </case>
                </outfitmatch>                
                <outfitmatch attachment="legs">
                    <case>
                        <caseparam type="equals">trousers</caseparam>
                        <action type="display-part">basic_pants</action>
                        <attributematch attribute="style">
                            <case>
                                <caseparam type="equals">burlap</caseparam>
                                <action type="display-part">basic_pants</action>
                            </case>
                            <case>
                                <caseparam type="equals">chain</caseparam>
                                <action type="display-part">basic_chain_pants</action>
                            </case>
                            <case>
                                <caseparam type="equals">ossyja</caseparam>
                                <action type="display-part">ossyja_pants</action>
                            </case>
                        </attributematch>
                    </case>
                </outfitmatch>                
            </case>
        </entitymatch>
    </entitymapping>
	
